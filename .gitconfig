[user]
	name = Hidekazu Kobayashi
	email = kobahide789@gmail.com
[core]
	excludesfile = ~/.gitignore
	pager = less -R
[color]
	ui = auto
[alias]
	st = status
	co = checkout
	ca = commit -a
	ps = push
	lg = log --stat --graph --decorate=full
	pl = pull
	delete-merged-branches = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
[push]
	default = simple
[include]
	path = .gitconfig.local
[init]
	defaultBranch = main
