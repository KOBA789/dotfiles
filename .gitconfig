[core]
	excludesfile = ~/.gitignore
	pager = less -R
[color]
	ui = auto
[alias]
	st = status
	co = checkout
	ca = commit -a
	ps = push
	lg = log --stat --graph --decorate=full
	pl = pull
[push]
	default = simple
[include]
	path = .gitconfig.local